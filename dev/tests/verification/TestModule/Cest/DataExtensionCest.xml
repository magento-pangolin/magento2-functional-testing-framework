<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../src/Magento/FunctionalTestingFramework/Test/etc/testSchema.xsd">
    <cest name="DataExtensionCest">
        <test name="DataExtensionTest">
            <fillField stepKey="inputReplace" selector="#selector" userInput="{{extensionData.firstname}}"/>
            <fillField stepKey="selectorReplace" selector="#{{extensionData.prefix}}" userInput="input"/>
            <dragAndDrop stepKey="selector12Replace" selector1="#{{extensionData.middlename}}" selector2="{{extensionData.lastname}}"/>
            <conditionalClick stepKey="dependentSelectorReplace" dependentSelector="#{{extensionData.firstname}}" selector="{{extensionData.suffix}}" visible="true"/>
        </test>
    </cest>
</config>